import java.util.ArrayList;

public class KitKat
{
    public static void main (String [] args) {
        // good stuff  
        int i = 7;
        
        // wrap
        Integer wrap = i;
        
        System.out.println (wrap);
        
        
        //unwrap
        int val = wrap * 3;
        
        System.out.print (val);
       
       
    }
}
