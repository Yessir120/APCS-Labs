//constructor example 

public class ConstructorRunner
{
  public static void main ( String[] args )
  {
      Turkey2 bird = new Turkey2(5);
      bird.print();
      
      Turkey2 big = new Turkey2(17, true);
      big.print();
      
      //make another Turkey
      //use the new constructor you created    
      Turkey2 bob = new Turkey (17, true, 3);
      bob.print();
  }
}